  "version": "0.1.0",
  "author": "Tyler Johnson <tyler@tylerjohnson.me>",
  "private": true,
  "homepage": "https://watchat.work",
  "main": "public/electron.js",
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.0.1",
    "gh-pages": "^2.0.1",
    "immer": "^2.1.4",
    "little-loader": "^0.2.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "typescript": "^3.4.1",
    "use-debounce": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "electron-dev": "PORT=6363 concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:\\$PORT && electron .\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -mwl -c.extraMetadata.main=build/electron.js -c.extraMetadata.homepage=./",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
